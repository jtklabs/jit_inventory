# Database
DATABASE_URL=postgresql://inventory:inventory_pass@db:5432/inventory

# Credential Management
CREDENTIAL_BACKEND=local  # 'local' for dev, 'aws' for prod
CREDENTIAL_PATH=/app/data/credentials

# AWS (for production)
AWS_REGION=us-east-1
AWS_SECRET_PREFIX=jt-inventory/

# SNMP Defaults
SNMP_TIMEOUT=5
SNMP_RETRIES=2

# Logging
LOG_LEVEL=INFO
